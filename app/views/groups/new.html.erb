<div class="new_group_container">
<%= form_with model: @group, method: :post do |f| %>

    <div class="field">
      <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "Name" %>
    </div>

    <div class="field">
      <%= f.text_field :icon, autofocus: true, autocomplete: "icon", placeholder: "Icon URL" %>
    </div>

    <div class="actions">
      <%= f.submit "Sign up" %>
    </div>
  <% end %>
</div>